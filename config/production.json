{
  "authentication": {
    "secret": "WEB_API_SECRET"
  },
  "backend": {
    "url": "BACKEND_API_URL"
  },
  "connections": {
    "annotationDispatch": {
      "url": "ANNOTATION_DISPATCH_API_URL"
    },
    "derivedDispatch": {
      "url": "DERIVED_DISPATCH_API_URL"
    },
    "noaaNWS": {
      "url": "NOAA_NWS_API_URL"
    }
  },
  "databases": {
    "influxdb": {
      "default": {
        "org": "DEFAULT_INFLUXDB2_ORG",
        "token": "DEFAULT_INFLUXDB2_TOKEN",
        "url": "DEFAULT_INFLUXDB2_URL"
      }
    },
    "mongodb": {
      "defaultLogger": {
        "level": "error"
      },
      "metadata": {
        "url": "MONGO_URL"
      }
    },
    "mysql": {
      "legacy": {
        "options": {
          "pool": {
            "max": 1
          }
        },
        "url": "LEGACY_MYSQL_URL"
      }
    }
  },
  "port": 8080,
  "services": {
    "influx_select": {
      "apis": {
        "default": {
          "url": "DEFAULT_INFLUXDB_API_URL"
        }
      }
    }
  }
}