{
  "name": "dendra-web-api",
  "version": "1.1.0",
  "description": "Dendra public facing REST API.",
  "license": "BSD-2-Clause-FreeBSD",
  "author": "J. Scott Smith <scott@newleafsolutionsinc.com>",
  "main": "dist/server/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/DendraScience/dendra-web-api.git"
  },
  "scripts": {
    "babel-server": "babel src/server -d dist/server",
    "build": "npm run build-server",
    "build-server": "npm run lint-server && npm run clean-server && npm run babel-server && npm run copy-views",
    "clean": "npm run clean-server",
    "clean-server": "rm -rf dist/server/*",
    "copy-views": "cp -R src/server/views dist/server",
    "lint": "eslint src",
    "lint-server": "eslint src/server",
    "lint-test": "eslint test",
    "lint-test-server": "eslint test/server",
    "update-schema": "rm -f schema/* && cp ../dendra-json-schema/schemas/draft-06-latest/*.json schema && cp ../dendra-json-schema/schemas/draft-06-latest/extended/*.json schema",
    "start": "node ./dist/server/main.js",
    "start:dev": "NODE_ENV=development npm start",
    "start:prod": "NODE_ENV=production npm start",
    "test": "npm run test-server",
    "test-server": "npm run lint-test-server && NODE_ENV=test mocha test/server --recursive --use_strict",
    "test-server:build": "npm run build-server && npm run test-server",
    "test-server:watch": "chokidar --debounce 2000 --initial 'src/server/**/*.js' 'test/server/**/*.js' -c 'npm run test-server:build'"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "dependencies": {
    "@dendra-science/api-hooks-common": "0.0.4",
    "@dendra-science/math": "0.0.1",
    "@dendra-science/utils": "0.0.1",
    "@feathersjs/authentication": "^2.1.7",
    "@feathersjs/authentication-jwt": "^2.0.1",
    "@feathersjs/authentication-local": "^1.2.1",
    "@feathersjs/configuration": "^2.0.0",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.3",
    "@feathersjs/feathers": "^3.1.6",
    "@feathersjs/rest-client": "^1.4.1",
    "@feathersjs/socketio": "^3.2.2",
    "ajv": "^5.5.2",
    "ajv-merge-patch": "^3.0.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "feathers-authentication-hooks": "^0.3.0",
    "feathers-hooks-common": "^4.14.3",
    "feathers-mongodb": "^3.2.0",
    "feathers-sequelize": "^3.1.2",
    "helmet": "^3.13.0",
    "mathjs": "^5.1.1",
    "mongodb": "^3.1.1",
    "mysql2": "^1.6.0",
    "passport": "^0.4.0",
    "request": "^2.87.0",
    "sequelize": "^4.38.0",
    "winston": "^2.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "feathers-memory": "^2.1.3",
    "mocha": "^5.2.0"
  },
  "optionalDependencies": {
    "fsevents": "^1.0.0"
  }
}
