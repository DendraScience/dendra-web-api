{
  "name": "dendro-web-api",
  "version": "0.0.7",
  "description": "Project Dendro REST API.",
  "license": "BSD-2-Clause-FreeBSD",
  "author": "J. Scott Smith <scott@newleafsolutionsinc.com>",
  "main": "dist/server/main.js",
  "scripts": {
    "babel-server": "babel src/server -d dist/server",
    "build": "npm run build-server",
    "build-server": "npm run lint-server && npm run clean-server && npm run babel-server && npm run copy-views",
    "clean": "npm run clean-server",
    "clean-server": "rm -rf dist/server/*",
    "copy-views": "cp -R src/server/views dist/server",
    "lint": "eslint src",
    "lint-server": "eslint src/server",
    "lint-test": "eslint test",
    "lint-test-server": "eslint test/server",
    "start": "node ./dist/server/main.js",
    "start:dev": "NODE_ENV=development npm start",
    "start:prod": "NODE_ENV=production npm start",
    "test": "npm run test-server",
    "test-server": "npm run lint-test-server && NODE_ENV=test mocha test/server --recursive --use_strict",
    "test-server:build": "npm run build-server && npm run test-server",
    "test-server:watch": "chokidar --initial 'src/server/**/*.js' 'test/server/**/*.js' -c 'npm run test-server:build'"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "dependencies": {
    "ajv": "^5.2.0",
    "babel-runtime": "^6.11.6",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "feathers": "^2.1.3",
    "feathers-authentication": "^1.2.4",
    "feathers-configuration": "^0.4.1",
    "feathers-errors": "^2.8.1",
    "feathers-hook-validation-jsonschema": "^1.0.1",
    "feathers-hooks": "^2.0.1",
    "feathers-hooks-common": "^3.5.1",
    "feathers-mongodb": "^2.9.0",
    "feathers-rest": "^1.7.3",
    "feathers-sequelize": "^2.0.1",
    "feathers-socketio": "^2.0.0",
    "influx": "^5.0.7",
    "mathjs": "^3.13.3",
    "mongodb": "^2.2.29",
    "mysql": "^2.11.1",
    "passport": "^0.3.2",
    "pug": "^2.0.0-rc.2",
    "request": "^2.81.0",
    "sequelize": "^3.30.4",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node6": "^0.4.0",
    "chai": "^4.0.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "feathers-memory": "^1.1.0",
    "mocha": "^3.4.2"
  },
  "optionalDependencies": {
    "fsevents": "^1.1.2"
  }
}
