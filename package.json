{
  "name": "dendra-web-api",
  "version": "0.1.1",
  "description": "Dendra public facing REST API.",
  "license": "BSD-2-Clause-FreeBSD",
  "author": "J. Scott Smith <scott@newleafsolutionsinc.com>",
  "main": "dist/server/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/DendraScience/dendra-web-api.git"
  },
  "scripts": {
    "babel-server": "babel src/server -d dist/server",
    "build": "npm run build-server",
    "build-server": "npm run lint-server && npm run clean-server && npm run babel-server && npm run copy-views",
    "clean": "npm run clean-server",
    "clean-server": "rm -rf dist/server/*",
    "copy-views": "cp -R src/server/views dist/server",
    "lint": "eslint src",
    "lint-server": "eslint src/server",
    "lint-test": "eslint test",
    "lint-test-server": "eslint test/server",
    "update-schema": "rm -f schema/* && cp ../../JSON/dendra-json-schema/schemas/draft-06-latest/*.json schema",
    "start": "node ./dist/server/main.js",
    "start:dev": "NODE_ENV=development npm start",
    "start:prod": "NODE_ENV=production npm start",
    "test": "npm run test-server",
    "test-server": "npm run lint-test-server && NODE_ENV=test mocha test/server --recursive --use_strict",
    "test-server:build": "npm run build-server && npm run test-server",
    "test-server:watch": "chokidar --initial 'src/server/**/*.js' 'test/server/**/*.js' -c 'npm run test-server:build'"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "dependencies": {
    "@dendra-science/api-hooks-common": "0.0.4",
    "@dendra-science/math": "0.0.1",
    "@dendra-science/utils": "0.0.1",
    "ajv": "^5.5.2",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "feathers": "^2.2.4",
    "feathers-authentication": "^1.3.1",
    "feathers-authentication-hooks": "^0.1.6",
    "feathers-authentication-jwt": "^0.3.2",
    "feathers-authentication-local": "^0.4.4",
    "feathers-configuration": "^0.4.2",
    "feathers-errors": "^2.9.2",
    "feathers-hooks": "^2.1.2",
    "feathers-hooks-common": "^3.10.0",
    "feathers-mongodb": "^2.9.1",
    "feathers-rest": "^1.8.1",
    "feathers-sequelize": "^2.4.0",
    "feathers-socketio": "^2.0.1",
    "mathjs": "^3.20.1",
    "mongodb": "^2.2.34",
    "mysql2": "^1.5.1",
    "passport": "^0.4.0",
    "pug": "^2.0.0-rc.4",
    "request": "^2.83.0",
    "sequelize": "^4.32.1",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.1",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "feathers-memory": "^1.3.1",
    "mocha": "^5.0.4"
  },
  "optionalDependencies": {
    "fsevents": "^1.0.0"
  }
}
