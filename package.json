{
  "name": "dendra-web-api",
  "version": "2.0.6",
  "description": "Dendra public facing REST API.",
  "license": "BSD-2-Clause-FreeBSD",
  "author": "J. Scott Smith <scott@newleafsolutionsinc.com>",
  "main": "dist/server/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/DendraScience/dendra-web-api.git"
  },
  "scripts": {
    "babel-server": "babel src/server -d dist/server",
    "build": "npm run build-server",
    "build-server": "npm run lint-server && npm run clean-server && npm run babel-server",
    "clean": "npm run clean-server",
    "clean-server": "rm -rf dist/server/*",
    "lint": "eslint src",
    "lint-server": "eslint src/server",
    "lint-test": "eslint test",
    "lint-test-server": "eslint test/server",
    "update-schema": "rm -f schema/* && cp ../dendra-json-schema/schemas/draft-07-latest/*.json schema && cp ../dendra-json-schema/schemas/draft-07-latest/extended/*.json schema",
    "start": "node ./dist/server/main.js",
    "start:dev": "NODE_ENV=development npm start",
    "start:prod": "NODE_ENV=production npm start",
    "debug": "DEBUG=express:* npm start",
    "test": "npm run test-server",
    "test-server": "npm run lint-test-server && NODE_ENV=test mocha test/server --recursive --use_strict",
    "test-server:build": "npm run build-server && npm run test-server",
    "test-server:watch": "chokidar --debounce 2000 --initial 'src/server/**/*.js' 'test/server/**/*.js' -c 'npm run test-server:build'"
  },
  "engines": {
    "node": ">=12.14"
  },
  "dependencies": {
    "@casl/ability": "^3.4.0",
    "@casl/mongoose": "^2.3.3",
    "@dendra-science/api-hooks-common": "^2.0.1",
    "@dendra-science/utils": "^1.0.0",
    "@feathersjs/authentication": "^2.1.16",
    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "^1.2.9",
    "@feathersjs/configuration": "^2.0.6",
    "@feathersjs/errors": "^3.3.6",
    "@feathersjs/express": "^1.3.1",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/rest-client": "^1.4.7",
    "@feathersjs/socketio": "^3.2.9",
    "ajv": "^6.12.4",
    "ajv-merge-patch": "^4.1.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "casl": "^1.1.0",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "feathers-authentication-hooks": "^0.3.1",
    "feathers-hooks-common": "^4.20.7",
    "feathers-mongodb": "^4.0.4",
    "feathers-sequelize": "^5.1.3",
    "helmet": "^3.23.3",
    "lodash": "^4.17.20",
    "mathjs": "^6.6.5",
    "mongodb": "^3.6.0",
    "mongoose": "^5.10.0",
    "mysql2": "^1.7.0",
    "passport": "^0.4.1",
    "poolifier": "^1.1.0",
    "sequelize": "^5.22.3",
    "sift": "^8.5.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^6.2.3",
    "prettier": "^2.1.0",
    "request": "^2.88.2"
  },
  "optionalDependencies": {}
}
